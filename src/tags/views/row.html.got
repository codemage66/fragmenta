{{ if not .tag.ID }}
    <tr class="data-table-head">
        <td>Id</td>
        <td>Updated</td>
        <td>Status</td>
        <td>Actions</td>
    </tr>
{{ else }}
    <tr {{ if odd .i }}class="odd"{{end}}>
        <td>{{ .tag.ID }}</td>
        <td>{{ time .tag.UpdatedAt }}</td>
        <td>{{ .tag.StatusDisplay }}</td>
        <td><a href="{{ .tag.UpdateURL }}">Edit Tags</a></td>
    </tr>
{{ end }}