<article>
<h1>Images</h1>

<form accept-charset="UTF-8" action="/images" method="get" class="filter_form">
      <input type="search" name="filter" class="right" placeholder="Search..." value="{{ .filter }}">
</form>

<table class="data-table">
    {{ $0 := . }}
    {{ template "images/views/tr.html.got" empty }}
    {{ range $i,$m := .images }}
       {{ set $0 "image" $i }}
       {{ set $0 "image" $m }}
       {{ template "images/views/tr.html.got" $0 }}
    {{ end }}
</table>
</article>