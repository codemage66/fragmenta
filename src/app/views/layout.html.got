<!DOCTYPE html>
<html lang="en">
<head>
    {{ template "app/views/meta.html.got" . }}
    {{ template "app/views/includes.html.got" . }}
</head>
<body>
    
    <header>
    {{ template "app/views/header.html.got" . }}
    </header>
  
    <article>
    {{ if .error }}
        <div class="error_box">{{.error}}</div>
    {{ end }}
    {{ .content }}
    </article>

    <footer>
    {{ template "app/views/footer.html.got" . }}
    </footer>

</body>
</html>