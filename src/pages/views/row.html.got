{{ if not .page.ID }}
    <tr class="data-table-head">
        <td>Id</td>
        <td>Updated</td>
        <td>Status</td>
        <td>Actions</td>
    </tr>
{{ else }}
    <tr {{ if odd .i }}class="odd"{{end}}>
        <td>{{ .page.ID }}</td>
        <td>{{ time .page.UpdatedAt }}</td>
        <td>{{ .page.StatusDisplay }}</td>
        <td><a href="{{ .page.UpdateURL }}">Edit Pages</a></td>
    </tr>
{{ end }}